<% for (let product of products) { %>
    <div class="wrap-modal1 js-modal-all js-modal<%= product._id %> p-t-60 p-b-20">
            <div class="overlay-modal1 js-hide-modal1"></div>
    
            <div class="container">
                <div class="bg0 p-t-60 p-b-30 p-lr-15-lg how-pos3-parent">
                    <button class="how-pos3 hov3 trans-04 js-hide-modal1">
                        <img src="images/icons/icon-close.png" alt="CLOSE">
                    </button>
    
                    <div class="row">
                        <div class="col-md-6 col-lg-7 p-b-30">
                            <div class="p-l-25 p-r-30 p-lr-0-lg">
                                <div class="wrap-slick3 flex-sb flex-w">
                                    <div class="wrap-slick3-dots"></div>
                                    <div class="wrap-slick3-arrows flex-sb-m flex-w"></div>
    
                                    <div class="slick3 gallery-lb">
                                        <div class="item-slick3" data-thumb="<%= product.image1Url %>">
                                            <div class="wrap-pic-w pos-relative">
                                                <img src="<%= product.image1Url %>" alt="IMG-PRODUCT">
    
                                                <a class="flex-c-m size-108 how-pos1 bor0 fs-16 cl10 bg0 hov-btn3 trans-04" href="<%= product.image1Url %>">
                                                    <i class="fa fa-expand"></i>
                                                </a>
                                            </div>
                                        </div>
    
                                        <% if (product.image2Url) { %>
                                        <div class="item-slick3" data-thumb="<%= product.image2Url %>">
                                            <div class="wrap-pic-w pos-relative">
                                                <img src="<%= product.image2Url %>" alt="IMG-PRODUCT">
    
                                                <a class="flex-c-m size-108 how-pos1 bor0 fs-16 cl10 bg0 hov-btn3 trans-04" href="images/product-detail-02.jpg">
                                                    <i class="fa fa-expand"></i>
                                                </a>
                                            </div>
                                        </div>
                                        <% } %>
    
                                        <% if (product.image3Url) { %>
                                        <div class="item-slick3" data-thumb="<%= product.image3Url %>">
                                            <div class="wrap-pic-w pos-relative">
                                                <img src="<%= product.image3Url %>" alt="IMG-PRODUCT">
    
                                                <a class="flex-c-m size-108 how-pos1 bor0 fs-16 cl10 bg0 hov-btn3 trans-04" href="images/product-detail-03.jpg">
                                                    <i class="fa fa-expand"></i>
                                                </a>
                                            </div>
                                        </div>
                                        <% } %>
                                    </div>
                                </div>
                            </div>
                        </div>
                        
                        <%- include('../includes/product-detail-div') %>
    
                    </div>
                </div>
            </div>
        </div>
<% } %>